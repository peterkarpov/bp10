@model ESN.Domain.Entities.Profile

@{
    Layout = null;
}

<h2>Filter</h2>

@using (Html.BeginForm("Filter", "ProfilesList", FormMethod.Get))
{
    <div class="form-group">

        <div class="form-group">
            <label>@Html.LabelFor(m => m.fName)</label>
            @Html.EditorFor(p => p.fName)
        </div>

        <div class="form-group">
            <label>@Html.LabelFor(m => m.lName)</label>
            @Html.EditorFor(p => p.lName)
        </div>




    </div>
    
    <button type="submit" class="btn btn-default center-block">
        Save Changes
    </button>
}
